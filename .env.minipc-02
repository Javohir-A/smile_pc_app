# .env.minipc-02 - Different mini PC for office area
# =============================================================================
# DEVICE IDENTIFICATION
# =============================================================================
DEVICE_NAME=minipc-office-02
DEVICE_LOCATION=office-area
DEVICE_TYPE=mini-pc

# =============================================================================
# CAMERA CONFIGURATION - Office cameras
# =============================================================================
RTSP_URLS=rtsp://admin:office456@192.168.100.30:554/stream,rtsp://admin:office456@192.168.100.31:554/stream,rtsp://admin:office456@192.168.100.32:554/stream

FALLBACK_URL=rtsp://admin:office456@192.168.100.35:554/stream

# Different settings for office environment
DETECTION_INTERVAL=4
EMOTION_DETECTION_INTERVAL=0.6
MIN_EMOTION_DURATION=2.0

# Office might have GUI for monitoring
ENABLE_GUI=true
DISPLAY_WIDTH=1920
DISPLAY_HEIGHT=1080

# =============================================================================
